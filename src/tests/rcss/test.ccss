body {
    font-family: "Arial, sans-serif";
    margin: 0;
    padding: 20px;
    background-color: #f5f5f5;
}

.test-section {
    margin-bottom: 30px;
    border: 1px solid #ddd;
    border-radius: 5px;
    background-color: white;
}

.test-section h3 {
    background-color: #eee;
    padding: 10px;
    margin: 0;
}

.test-content {
    padding: 20px;
}

.feature-label {
    font-size: 12px;
    color: #666;
    margin-bottom: 5px;
}